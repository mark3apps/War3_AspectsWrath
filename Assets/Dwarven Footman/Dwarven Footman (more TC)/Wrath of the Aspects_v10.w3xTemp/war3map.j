		mapAI:CleanUp(pickedHero)
    else
        mapAI:STATERevived(pickedHero)
    end

	
    if (AI_Loop >= mapAI.count) then
        AI_Loop = 1
    else
        AI_Loop = (AI_Loop + 1)
    end
end

