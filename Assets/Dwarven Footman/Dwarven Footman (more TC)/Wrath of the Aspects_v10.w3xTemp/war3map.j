		function self:ACTIONtravelToHeal(i)
			local hero = self[i]

			local healDistance = 100000000.00
			local healDistanceNew = 0.00
			local unitX
			local unitY
			local u
			local g = CreateGroup()


			GroupAddGroup(udg_UNIT_Healing[hero.teamNumber], g)
			while true do
