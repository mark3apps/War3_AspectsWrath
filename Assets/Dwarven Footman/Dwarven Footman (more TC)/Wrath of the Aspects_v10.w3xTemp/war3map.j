function Trig_Event_Count_Actions takes nothing returns nothing
    set udg_TEMP_UnitGroup = GetUnitsInRectAll(gg_rct_Center_Events)
    call ForGroupBJ( udg_TEMP_UnitGroup, function Trig_Event_Count_Func002A )
    //! beginusercode
